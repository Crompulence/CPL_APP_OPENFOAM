FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          (liquid vapour); // FC-72

sigma           0;

liquid
{
    transportModel  Newtonian;
    nu              1.7;
    rho             0.7;

    Cp              1;
    Cv              1;   // assume Cp for liquid
    kappa           1;
    hf              0;
}

vapour
{
    transportModel  Newtonian;
    nu              1.7;
    rho             0.02;

    Cp              1;      // FC72 vapour
    Cv              1;      // Cv = Cp - R/w
    kappa           1;       // FC72 vapour
    hf              0;
}

Prt             0.7;

nu              nu [ 0 2 -1 0 0 0 0 ] 1.7;

// ************************************************************************* //
