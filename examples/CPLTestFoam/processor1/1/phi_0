FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       surfaceScalarField;
    location    "1";
    object      phi_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

oriented        oriented;

internalField   uniform 0;

boundaryField
{
    movingWall
    {
        type            calculated;
        value           uniform 0;
    }
    CPLReceiveMD
    {
        type            calculated;
        value           uniform 0;
    }
    streamwiseIn
    {
        type            cyclic;
        value           nonuniform List<scalar> 0();
    }
    streamwiseOut
    {
        type            cyclic;
        value           nonuniform List<scalar> 0();
    }
    front
    {
        type            cyclic;
        value           uniform 0;
    }
    back
    {
        type            cyclic;
        value           uniform 0;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to0throughstreamwiseOut
    {
        type            processorCyclic;
        value           uniform 0;
    }
