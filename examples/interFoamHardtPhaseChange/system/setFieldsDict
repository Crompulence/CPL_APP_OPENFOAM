defaultFieldValues
(
    volScalarFieldValue alpha.liquid 1.0
);

regions
(
    boxToCell
    {
        box (0.0 270.0 -5.0) (476.22031559046 476.22031559046 5.0);
        fieldValues
        (
            volScalarFieldValue alpha.liquid 0.0
        );
    }
);


